<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PackageId>OperationsResearch</PackageId>
    <Description>A library enabling Operations Research for F# applications.</Description>
    <Authors>Matthew Moore</Authors>
    <Copyright>Matthew Moore</Copyright>
    <Readme>docs\README.md</Readme>
    <PackageTags>F#, Operations Research, Mathematics, Optimization</PackageTags>
    <RepositoryUrl>https://github.com/acco32/Operations-Research</RepositoryUrl>
    <License>https://github.com/acco32/Operations-Research/blob/master/LICENCE</License>
    <PackageIconUrl></PackageIconUrl>
    <PackageOutputPath>../packages</PackageOutputPath>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Files>
      <File src="../VERSION" />
      <File src="../LICENCE" />
      <File src="nuget.md" target="docs/README.md" />
    </Files>
  </PropertyGroup>
</Project>
